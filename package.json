{
  "name": "kysely",
  "version": "0.2.4",
  "description": "Type safe SQL query builder",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "npm run test:integration && npm run test:typings",
    "test:integration": "mocha --timeout 5000 -r ts-node/register test/**/*.test.ts",
    "test:typings": "npm run build && tsd test/typings",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "rm -rf lib && tsc",
    "docs:build": "typedoc  --disableSources --excludePrivate --excludeInternal src/index.ts && npm run docs:postprocess",
    "docs:postprocess": "node post-process-docs.js",
    "prepublish": "npm run build"
  },
  "author": "Sami Koskim√§ki <sami@jakso.me>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/pg": "^8.6.1",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "mocha": "^9.1.1",
    "pg": "^8.7.1",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.0",
    "tsd": "^0.17.0",
    "typedoc": "^0.22.3",
    "typescript": "4.4.3"
  }
}

{
  "name": "kysely",
  "version": "0.3.8",
  "description": "Type safe SQL query builder",
  "repository": {
    "type": "git",
    "url": "git://github.com/koskimas/kysely.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test:build": "rm -rf test/lib && tsc -p test",
    "clean": "rm -rf dist && rm -rf test/dist",
    "test": "npm run clean && npm run build:cjs && npm run test:build && npm run test:integration && npm run test:typings",
    "test:integration": "mocha --timeout 5000 test/dist/**/*.test.js",
    "test:typings": "tsd test/typings",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && ./module-fixup.sh",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "docs:build": "typedoc --excludePrivate --excludeInternal src/index.ts && npm run docs:postprocess",
    "docs:postprocess": "node post-process-docs.js",
    "prepublish": "npm run build"
  },
  "author": "Sami Koskim√§ki <sami@jakso.me>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^9.0.0",
    "@types/pg": "^8.6.1",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "mocha": "^9.1.1",
    "pg": "^8.7.1",
    "prettier": "^2.4.0",
    "tsd": "^0.17.0",
    "typedoc": "^0.22.4",
    "typescript": "4.4.3"
  }
}
